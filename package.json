{
  "private": true,
  "name": "emoji-search",
  "productionName": "Emoji Searcher: find the emoji that echoes your mind",
  "description": "A modern emoji searcher.",
  "version": "0.0.0",
  "main": "src/index.js",
  "repository": {},
  "scripts": {
    "test": "echo lol",
    "lint": "xo",
    "dev": "poi",
    "build": "poi build",
    "surge": "npm run build && surge -p dist -d emoji-search.surge.sh",
    "deploy": "npm run lint && npm run build && gh-pages -d dist"
  },
  "xo": {
    "extends": "vue-app",
    "extensions": [
      "vue",
      "js"
    ],
    "rules": {
      "unicorn/filename-case": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0
    }
  },
  "devDependencies": {
    "babel-plugin-component": "^0.9.1",
    "eslint-config-vue-app": "^1.2.1",
    "gh-pages": "^0.12.0",
    "poi": "^8.0.0-rc.1",
    "xo": "^0.18.1"
  },
  "license": "MIT",
  "dependencies": {
    "clipboard": "^1.6.1",
    "element-ui": "^1.2.4",
    "emojilib": "^2.1.0",
    "fuse.js": "^2.6.2",
    "lodash.debounce": "^4.0.8",
    "native-toast": "^1.1.0",
    "offline-plugin": "^4.6.1",
    "unfetch": "^2.1.2",
    "vue-github-badge": "^1.0.0"
  }
}
